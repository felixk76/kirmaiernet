{
  "name": "website",
  "version": "1.0.0",
  "description": "Repository for my personal page www.kirmaier.net",
  "main": ".eleventy.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/felixkirmaier/website.git"
  },
  "keywords": [],
  "author": "Felix Kirmaier",
  "license": "ISC",
  "homepage": "https://bitbucket.org/felixkirmaier/website#readme",
  "scripts": {
    "copyjs": "grunt copy:vendorJS",
    "build:dev": "npm run eleventy:dev && npm run workbox-inject && npm run parcel:dev",
    "build:prod": "npm run eleventy:prod && npm run workbox-inject && npm run parcel:prod && workbox copyLibraries dist/js/vendors/",
    "eleventy:dev": "eleventy --serve",
    "eleventy:prod": "eleventy",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix",
    "eslint-nibble": "eslint-nibble .",
    "parcel:dev": "NODE_ENV=development parcel build src/sw.js --no-minify",
    "parcel:prod": "NODE_ENV=production parcel build src/sw.js --no-source-maps",
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "npm run workbox-copy",
    "start": "npx eleventy --serve",
    "stylelint-find-rules": "stylelint-find-new-rules",
    "stylelint": "stylelint 'src/Scss/**/*.scss' --formatter verbose",
    "stylelint-fix": "stylelint 'src/Scss/**/*.scss'  --fix --formatter verbose",
    "stylelint-config": "stylelint 'srcScss/style.scss' --print-config",
    "build": "grunt build && npm run build:dev",
    "deploy": "grunt build && npm run build:prod",
    "workbox-copy": "workbox copyLibraries src/js/vendors/ && workbox copyLibraries dist/js/vendors/",
    "workbox-inject": "workbox injectManifest workbox-config.js"
  },
  "devDependencies": {
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "autoprefixer": "^9.x.x",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "buffer-crc32": "^0.2.13",
    "eslint": "^7.19.0",
    "eslint-config-felixk76": "^1.0.15",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "grunt": "^1.3.0",
    "grunt-contrib-compress": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-eslint": "^23.0.0",
    "grunt-notify": "^0.4.5",
    "grunt-nunjucks-2-html": "^3.1.0",
    "grunt-postcss": "^0.9.0",
    "grunt-rsync": "^3.0.0",
    "grunt-sass": "^3.1.0",
    "grunt-stylelint": "^0.16.0",
    "grunt-terser": "^2.0.0",
    "grunt-zopfli-native": "^2.0.0",
    "iltorb": "^2.4.4",
    "load-grunt-config": "^3.0.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.2",
    "parcel-bundler": "^1.12.4",
    "postcss": "^8.2.13",
    "postcss-critical-split": "^2.5.3",
    "postcss-pxtorem": "^5.1.1",
    "stylelint": "^13.10.0",
    "stylelint-config-felixk76": "0.0.4",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "time-grunt": "^2.0.0"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@navillus/eleventy-plugin-inline-css": "^1.0.3",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eslint-nibble": "^6.1.0",
    "fg-loadcss": "^3.1.0",
    "husky": "^5.0.9",
    "instant.page": "^5.1.0",
    "intersection-observer": "^0.12.0",
    "lazysizes": "^5.3.0",
    "lint-staged": "^10.5.4",
    "mrm": "^2.5.18",
    "picturefill": "^3.0.3",
    "stylelint-find-new-rules": "^3.0.1",
    "workbox-build": "^6.1.0",
    "workbox-cli": "^6.1.0"
  },
  "optionalDependencies": {
    "eslint-nibble": "^6.1.0",
    "husky": "^5.0.9",
    "mrm": "^2.5.18",
    "lint-staged": "^10.5.4",
    "stylelint-find-new-rules": "^3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 2 version",
    "not ie > 0",
    "not ie_mob > 0",
    "> 5%"
  ],
  "resolutions": {
    "lodash": "4.17.20"
  }
}
